{
  "version": "1.0.0-*",
  "compilationOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "Microsoft.AspNet.IISPlatformHandler": "1.0.0-rc1-final",
    "Microsoft.AspNet.Mvc": "6.0.0-rc1-final",
    "Microsoft.AspNet.Mvc.Razor": "6.0.0-rc1-final",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-final",
    "Microsoft.AspNet.StaticFiles": "1.0.0-rc1-final",
    "Microsoft.AspNet.Tooling.Razor": "1.0.0-rc1-final",
    "Microsoft.AspNet.Mvc.TagHelpers": "6.0.0-rc1-final"
  },

  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel"
  },

  "frameworks": {
    "dnx46": {
      "dependencies": {
        "Chatterbox.Model": "1.0.0-*"
      }
    }
  },

  "Data": {
    "DefaultConnection": {
      "ConnectionString": "metadata=res://*/Models.ChatterboxModel.csdl|res://*/Models.ChatterboxModel.ssdl|res://*/Models.ChatterboxModel.msl;provider=System.Data.SqlClient;provider connection string=';data source=.;initial catalog=Chatterbox;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework';"
    }
  },

  "exclude": [
    "wwwroot",
    "node_modules"
  ],
  "publishExclude": [
    "**.user",
    "**.vspscc"
  ]
}
